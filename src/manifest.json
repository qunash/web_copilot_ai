{
    "manifest_version": 3,
    "name": "Web Copilot AI",
    "version": "1.0",
    "description": "AI-powered web assistant",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "service_worker.js",
      "type": "module"
    },
    "action": {
      "default_popup": "index.html"
    },
    "host_permissions": [
      "*://*/*"
    ]
}
